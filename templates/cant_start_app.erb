<html>
  <head>
    <title>Can't start application <%= @app_name %></title>
  </head>
  <body>
    <h1>Can't start application <q><%= @app_name %></q></h1>
    <p>Please look at <code><%= @app.log_path %></code> for details</p>
    <% unless error.nil? %>
      <code>
        <%= error.message %>
      </code>
    <% end %>
  </body>
</html>
